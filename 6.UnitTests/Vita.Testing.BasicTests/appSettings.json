{
  "ServerTypeForTestExplorer": "Postgres",
  "useBatchMode": "true",
  "ServerTypesForConsoleRun": "MsSql,MySql,Postgres,Oracle,SQLite",
  // Connection strings for different server types
  "MsSqlConnectionString": "Data Source=.\\MSSQL2017;Initial Catalog=VitaTest;Integrated Security=True",
  "MySqlConnectionString": "Server=localhost; Uid=root; Pwd=_pass; Old Guids=true;SslMode=none",
  // Note: for Postgres with windows auth, set Host to IP address, not localhost; you need to create Login Role in PG server matching your user name, pwd does not matter
  // or just create testuser
  "PostgresConnectionString": "Host=127.0.0.1;Port=5432;Database=VitaTest;Pooling=true;userid=postgres;password=_pass",

  // For MS SQLite provider
  "SQLiteConnectionString": "Data Source={bin}\\VitaTestSQLite.db;Mode=ReadWriteCreate",

  "OracleConnectionString": "Data Source=(DESCRIPTION = (ADDRESS=(PROTOCOL = TCP)(HOST =localhost)(PORT = 1521))(CONNECT_DATA =(SID = ORCL))); User Id=system;Password=pass"
}    

